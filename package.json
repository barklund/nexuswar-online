{
  "name": "nexuswar-online",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy:s3": "aws s3 sync out s3://static.nexuswar.online --acl public-read",
    "deploy:invalidate": "aws cloudfront create-invalidation --distribution-id E1UFOX55D8XMS6 --paths '/*'",
    "deploy": "yarn build && yarn next export && yarn deploy:s3 && yarn invalidate"
  },
  "dependencies": {
    "next": "9.2.2",
    "next-images": "^1.3.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "babel-plugin-styled-components": "^1.10.7",
    "typescript": "^3.8.3"
  }
}
